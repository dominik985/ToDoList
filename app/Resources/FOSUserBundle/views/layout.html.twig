<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <script src="https://use.fontawesome.com/890ef4f0ad.js"></script>
    {% block stylesheets %}

    {% endblock %}
</head>
<body>

{% if app.request.hasPreviousSession %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="flash-{{ type }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
{% endif %}

<div>
    {% block fos_user_content %}
    {% endblock fos_user_content %}
</div>
</body>
</html>
